# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                                   controllers.FinanalysisController.index
GET     /param/:num                         controllers.FinanalysisController.showparam(num: Int)
GET     /list/:name                         controllers.FinanalysisController.listofthings(name: String)
GET     /assets/chart.js                    controllers.FinanalysisController.charts
GET     /assets/chart.js                    controllers.FinanalysisController.charts
POST    /                                   controllers.FinanalysisController.upload
GET     /books                              controllers.FinanalysisController.listBooks
GET     /react                              controllers.FinanalysisController.react
GET     /statement                          controllers.FinanalysisController.statement


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file              controllers.Assets.versioned(path="/public", file: Asset)

GET    /webjars/*file               controllers.WebJarAssets.at(file)