# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                                   finanalysis.controllers.FinanalysisController.index
GET     /param/:num                         finanalysis.controllers.FinanalysisController.showparam(num: Int)
GET     /list/:name                         finanalysis.controllers.FinanalysisController.listofthings(name: String)
POST    /                                   finanalysis.controllers.FinanalysisController.upload
GET     /react                              finanalysis.controllers.FinanalysisController.react
GET     /statement                          finanalysis.controllers.FinanalysisController.statement


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

GET    /webjars/*file               controllers.WebJarAssets.at(file)